      <head>Schema Specifics</head>
      <schemaSpec ident="GREG" start="TEI">
        <!-- Four Base TEI Modules -->
        <moduleRef key="tei"/>
        <moduleRef key="core"
          include="graphic head hi item label lb list p pb q ref resp respStmt title"/>
        <moduleRef key="corpus" include="settingDesc"/>
        <moduleRef key="header"
          include="abstract availability category catDesc catRef classDecl editionStmt encodingDesc fileDesc handNote idno licence profileDesc publicationStmt rendition revisionDesc samplingDecl seriesStmt sourceDesc tagsDecl taxonomy teiHeader textClass titleStmt"/>
        <moduleRef key="textstructure"
          include="body div TEI text"/>
        <!-- Other TEI Modules -->
      </schemaSpec>
    </back>
